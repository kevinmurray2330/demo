<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="222.0" prefWidth="387.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="aydin.demo.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="usernameTextField" promptText="enter username" />
   <TextField fx:id="passwordTextField" promptText="enter password" />

    <Button fx:id="signinButton" onAction="#onSigninButtonClick" text="Signin" />
</VBox>
